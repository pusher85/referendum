			 </div>
			 </section>
			<% end %>



			<% flash.each do |key, value| %>
				<hr>
						<section>
							<div class="container">
				        <div class="alert alert-<%= key %>">
				        	<button data-dismiss="alert" class="close">
										&times;
									</button>
									<i class="fa fa-exclamation-triangle"></i>
				        	<strong>Attenzione!</strong> <%= value %>
				        </div>
				      </div>
